o, display, setIntroDisplay }) {\n\n  return (\n    <MovieIntroContainer>\n      <MovieIntro key={intro.id} movie={intro} display={display} setIntroDisplay={setIntroDisplay} />\n    </MovieIntroContainer>\n  );\n}",{"ruleId":"142","replacedBy":"143"},{"ruleId":"144","replacedBy":"145"},{"ruleId":"142","replacedBy":"146"},{"ruleId":"144","replacedBy":"147"},{"ruleId":"142","replacedBy":"148"},{"ruleId":"144","replacedBy":"149"},{"ruleId":"142","replacedBy":"150"},{"ruleId":"144","replacedBy":"151"},{"ruleId":"152","severity":1,"message":"153","line":56,"column":10,"nodeType":"154","messageId":"155","endLine":56,"endColumn":17},{"ruleId":"152","severity":1,"message":"156","line":56,"column":19,"nodeType":"154","messageId":"155","endLine":56,"endColumn":34},{"ruleId":"152","severity":1,"message":"157","line":1,"column":10,"nodeType":"154","messageId":"155","endLine":1,"endColumn":19},{"ruleId":"152","severity":1,"message":"158","line":1,"column":21,"nodeType":"154","messageId":"155","endLine":1,"endColumn":29},{"ruleId":"152","severity":1,"message":"159","line":2,"column":10,"nodeType":"154","messageId":"155","endLine":2,"endColumn":25},{"ruleId":"152","severity":1,"message":"160","line":4,"column":10,"nodeType":"154","messageId":"155","endLine":4,"endColumn":14},{"ruleId":"161","severity":1,"message":"162","line":178,"column":70,"nodeType":"163","endLine":178,"endColumn":123},"no-native-reassign",["164"],"no-negated-in-lhs",["165"],["164"],["165"],["164"],["165"],["164"],["165"],"no-unused-vars","'display' is assigned a value but never used.","Identifier","unusedVar","'setIntroDisplay' is assigned a value but never used.","'useEffect' is defined but never used.","'useState' is defined but never used.","'getLatestMovies' is defined but never used.","'Link' is defined but never used.","jsx-a11y/iframe-has-title","<iframe> elements must have a unique title property.","JSXOpeningElement","no-global-assign","no-unsafe-negation"]